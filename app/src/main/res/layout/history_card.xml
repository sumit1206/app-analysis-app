<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/margin10"
        android:paddingEnd="@dimen/margin10"
        android:background="@null"
        android:padding="@dimen/margin5"
        android:layout_margin="@dimen/margin5">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/margin5"
            android:orientation="vertical">

            <TextView
                android:id="@+id/history_card_app_name"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/white"
                android:textAlignment="center"
                android:textSize="@dimen/text_size14"
                android:textStyle="bold"
                android:drawablePadding="@dimen/margin10"
                android:text="@string/app_name"
                android:layout_weight="1"/>

            <TextView
                android:id="@+id/history_card_date_time"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textSize="@dimen/text_size12"
                android:textAlignment="center"
                android:drawablePadding="@dimen/margin10"
                android:text="@string/app_name"
                android:textColor="@color/text_color"
                android:layout_weight="1"/>
        </LinearLayout>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/margin5">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size16"
                    android:drawablePadding="@dimen/margin10"
                    android:drawableStart="@drawable/audiotrack_black"/>

                <TextView
                    android:id="@+id/history_card_audio_uploaded"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size12"
                    android:textAllCaps="true"
                    android:textAlignment="textStart"
                    android:paddingStart="@dimen/margin5" />
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="horizontal">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableStart="@drawable/receipt_black"
                    android:drawablePadding="@dimen/margin10"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size16" />

                <TextView
                    android:id="@+id/history_card_csv_uploaded"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:textColor="@color/text_color"
                    android:textSize="@dimen/text_size12"
                    android:textAllCaps="true"
                    android:textAlignment="textStart"
                    android:paddingStart="@dimen/margin5" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/history_card_session_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:textColor="@color/text_color"
                android:textAlignment="textStart"
                android:layout_weight="1"
                android:layout_marginTop="@dimen/margin5"
                android:ellipsize="end"
                android:gravity="start|center"
                android:drawableStart="@drawable/info_outline_black"
                android:drawablePadding="@dimen/margin10"
                android:text="@string/app_name" />

            <ImageView
                android:id="@+id/history_card_delete"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/ic_delete_black_24dp"
                android:rotation="30"/>
        </LinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="@dimen/margin5"
            android:background="@color/view_color"/>
    </LinearLayout>
</LinearLayout>